<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabet|alphabetAutomaticShifted|alphabetShiftLocked">
            <Key latin:additionalMoreKeys=":" latin:keySpec=";" />
            <Key latin:additionalMoreKeys="&quot;" latin:keySpec="'" latin:moreKeys="!fixedColumnOrder!4,!text/double_quotes,%,!text/single_quotes" />
        </case>
        <default>
            <Key latin:keySpec=":" />
            <Key latin:keySpec="&quot;" latin:moreKeys="!fixedColumnOrder!3,!text/double_quotes,!text/single_quotes" />
        </default>
    </switch>
</merge>