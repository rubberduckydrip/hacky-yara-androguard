<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <key-style latin:styleName="navigateMoreKeysStyle" />
    <key-style latin:backgroundType="action" latin:keyActionFlags="noKeyPreview" latin:keyLabelFlags="followKeyLabelRatio|autoXScale|preserveCase|followFunctionalTextColor" latin:keySpec="!icon/enter_key|!code/key_enter" latin:parentStyle="navigateMoreKeysStyle" latin:styleName="defaultEnterKeyStyle" />
    <include latin:keyboardLayout="@xml/key_styles_actions" />
    <switch>
        <case latin:isMultiLine="true" latin:keyboardLayoutSetElement="alphabetManualShifted">
            <key-style latin:keySpec="!icon/enter_key|!code/key_shift_enter" latin:parentStyle="defaultEnterKeyStyle" latin:styleName="enterKeyStyle" />
        </case>
        <case latin:imeAction="actionGo">
            <key-style latin:parentStyle="goActionKeyStyle" latin:styleName="enterKeyStyle" />
        </case>
        <case latin:imeAction="actionNext">
            <key-style latin:parentStyle="nextActionKeyStyle" latin:styleName="enterKeyStyle" />
        </case>
        <case latin:imeAction="actionPrevious">
            <key-style latin:parentStyle="previousActionKeyStyle" latin:styleName="enterKeyStyle" />
        </case>
        <case latin:imeAction="actionDone">
            <key-style latin:parentStyle="doneActionKeyStyle" latin:styleName="enterKeyStyle" />
        </case>
        <case latin:imeAction="actionSend">
            <key-style latin:parentStyle="sendActionKeyStyle" latin:styleName="enterKeyStyle" />
        </case>
        <case latin:imeAction="actionSearch">
            <key-style latin:parentStyle="searchActionKeyStyle" latin:styleName="enterKeyStyle" />
        </case>
        <case latin:imeAction="actionCustomLabel">
            <key-style latin:parentStyle="customLabelActionKeyStyle" latin:styleName="enterKeyStyle" />
        </case>
        <default>
            <key-style latin:parentStyle="defaultEnterKeyStyle" latin:styleName="enterKeyStyle" />
        </default>
    </switch>
</merge>