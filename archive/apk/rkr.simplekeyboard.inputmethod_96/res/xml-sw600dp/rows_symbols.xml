<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <include latin:keyboardLayout="@xml/key_styles_common" />
    <include latin:keyboardLayout="@xml/key_styles_currency" />
    <Row latin:keyWidth="9.000004%p">
        <switch>
            <case latin:languageCode="bn">
                <include latin:keyboardLayout="@xml/rowkeys_bengali_numbers" />
            </case>
            <default>
                <include latin:keyboardLayout="@xml/rowkeys_symbols1" />
            </default>
        </switch>
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="9.000004%p">
        <include latin:keyXPos="4.4999957%p" latin:keyboardLayout="@xml/rowkeys_symbols2" />
        <Key latin:keyStyle="enterKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="9.000004%p">
        <Key latin:keyStyle="toMoreSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:keySpec="\\" />
        <Key latin:keySpec="=" />
        <include latin:keyboardLayout="@xml/rowkeys_symbols3" />
        <Key latin:keyStyle="toMoreSymbolKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:backgroundType="functional" latin:keyWidth="9.000004%p">
        <Key latin:keyStyle="toAlphaKeyStyle" latin:keyWidth="10.000002%p" />
        <include latin:keyboardLayout="@xml/row_symbols4" />
    </Row>
</merge>