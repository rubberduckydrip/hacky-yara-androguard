<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <include latin:keyboardLayout="@xml/key_styles_common" />
    <Row latin:keyLabelFlags="fontNormal" latin:keyWidth="7.500005%p">
        <include latin:keyboardLayout="@xml/rowkeys_thai1" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyLabelFlags="fontNormal" latin:keyWidth="7.500005%p">
        <include latin:keyXPos="2.4999976%p" latin:keyboardLayout="@xml/rowkeys_thai2" />
        <include latin:keyboardLayout="@xml/key_thai_kho_khuat" />
    </Row>
    <Row latin:keyLabelFlags="fontNormal" latin:keyWidth="7.500005%p">
        <include latin:keyXPos="4.999995%p" latin:keyboardLayout="@xml/rowkeys_thai3" />
        <Key latin:keyStyle="enterKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyLabelFlags="fontNormal" latin:keyWidth="7.500005%p">
        <Key latin:keyStyle="shiftKeyStyle" latin:keyWidth="10.000002%p" />
        <include latin:keyboardLayout="@xml/rowkeys_thai4" />
        <switch>
            <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked">
                <Spacer />
            </case>
            <default>
                <include latin:keyboardLayout="@xml/keys_exclamation_question" />
            </default>
        </switch>
    </Row>
    <include latin:keyboardLayout="@xml/row_qwerty4" />
</merge>