<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:showExtraChars="true">
            <Key latin:additionalMoreKeys="*" latin:keyHintLabel="*" latin:keySpec="w" latin:moreKeys="!text/morekeys_w" />
            <Key latin:additionalMoreKeys="&quot;" latin:keyHintLabel="&quot;" latin:keySpec="x" />
            <Key latin:additionalMoreKeys="'" latin:keyHintLabel="'" latin:keySpec="c" latin:moreKeys="!text/morekeys_c" />
            <Key latin:additionalMoreKeys=":" latin:keyHintLabel=":" latin:keySpec="v" latin:moreKeys="!text/morekeys_v" />
            <Key latin:additionalMoreKeys=";" latin:keyHintLabel=";" latin:keySpec="b" />
            <Key latin:additionalMoreKeys="!" latin:keyHintLabel="!" latin:keySpec="n" latin:moreKeys="!text/morekeys_n" />
        </case>
        <default>
            <Key latin:keySpec="w" latin:moreKeys="!text/morekeys_w" />
            <Key latin:keySpec="x" />
            <Key latin:keySpec="c" latin:moreKeys="!text/morekeys_c" />
            <Key latin:keySpec="v" latin:moreKeys="!text/morekeys_v" />
            <Key latin:keySpec="b" />
            <Key latin:keySpec="n" latin:moreKeys="!text/morekeys_n" />
        </default>
    </switch>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted">
            <Key latin:keySpec="\?" />
        </case>
        <default>
            <Key latin:keySpec="'" latin:moreKeys="!text/morekeys_single_quote" />
        </default>
    </switch>
</merge>