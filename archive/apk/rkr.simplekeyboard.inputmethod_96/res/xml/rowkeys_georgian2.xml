<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked">
            <Key latin:keySpec="A" />
            <Key latin:keySpec="შ" />
            <Key latin:keySpec="D" />
            <Key latin:keySpec="F" />
            <Key latin:keySpec="G" />
            <Key latin:keySpec="H" />
            <Key latin:keySpec="ჟ" />
            <Key latin:keySpec="K" />
            <Key latin:keySpec="L" />
        </case>
        <default>
            <Key latin:keySpec="ა" latin:moreKeys="ჺ" />
            <Key latin:keySpec="ს" />
            <Key latin:keySpec="დ" />
            <Key latin:keySpec="ფ" latin:moreKeys="ჶ" />
            <Key latin:keySpec="გ" latin:moreKeys="ჹ" />
            <Key latin:keySpec="ჰ" latin:moreKeys="ჵ" />
            <Key latin:keySpec="ჯ" latin:moreKeys="ჷ" />
            <Key latin:keySpec="კ" />
            <Key latin:keySpec="ლ" />
        </default>
    </switch>
</merge>