<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabet|alphabetAutomaticShifted|alphabetShiftLocked">
            <Key latin:additionalMoreKeys="&lt;" latin:keySpec="," />
            <Key latin:additionalMoreKeys=">" latin:keySpec="." />
            <Key latin:additionalMoreKeys="\?" latin:keySpec="/" latin:moreKeys="!text/morekeys_question" />
        </case>
        <default>
            <Key latin:keySpec="&lt;" latin:moreKeys="!fixedColumnOrder!3,‹,≤,«" />
            <Key latin:keySpec=">" latin:moreKeys="!fixedColumnOrder!3,›,≥,»" />
            <Key latin:keySpec="\?" latin:moreKeys="!text/morekeys_question" />
        </default>
    </switch>
</merge>