<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:languageCode="fa">
            <Key latin:keyHintLabel="\@" latin:keySpec="٬" latin:moreKeys="\@" />
            <Key latin:keyHintLabel="\#" latin:keySpec="٫" latin:moreKeys="\#" />
        </case>
        <default>
            <Key latin:keySpec="\@" />
            <Key latin:keySpec="\#" />
        </default>
    </switch>
    <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keyStyle="currencyKeyStyle" />
    <Key latin:keySpec="!text/keyspec_symbols_percent" latin:moreKeys="!text/morekeys_symbols_percent" />
    <Key latin:keySpec="&amp;" />
    <Key latin:keySpec="-" latin:moreKeys="_,–,—,·" />
    <Key latin:keySpec="+" latin:moreKeys="!text/morekeys_plus" />
    <include latin:keyboardLayout="@xml/keys_parentheses" />
</merge>