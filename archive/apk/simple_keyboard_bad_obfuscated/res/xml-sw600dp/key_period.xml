<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSet="dvorak">
            <Key latin:keySpec="\?" latin:moreKeys="!text/morekeys_tablet_period,!text/morekeys_question" />
        </case>
        <default>
            <Key latin:backgroundType="functional" latin:keyHintLabel="!text/keyhintlabel_tablet_period" latin:keyLabelFlags="hasPopupHint" latin:keySpec="!text/keyspec_tablet_period" latin:keyStyle="hasShiftedLetterHintStyle" latin:moreKeys="!text/morekeys_tablet_period" />
        </default>
    </switch>
</merge>