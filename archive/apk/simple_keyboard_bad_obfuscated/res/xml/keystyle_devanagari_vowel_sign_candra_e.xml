<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSet="hindi_compact">
            <key-style latin:moreKeys="ॅ" latin:styleName="moreKeysDevanagariVowelSignCandraE" />
        </case>
        <case latin:keyboardLayoutSet="marathi">
            <key-style latin:moreKeys="ऍ" latin:styleName="moreKeysDevanagariVowelSignCandraE" />
        </case>
        <default>
            <key-style latin:styleName="moreKeysDevanagariVowelSignCandraE" />
        </default>
    </switch>
    <key-style latin:keySpec="ॅ" latin:parentStyle="moreKeysDevanagariVowelSignCandraE" latin:styleName="baseKeyDevanagariVowelSignCandraE" />
</merge>