<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted">
            <Key latin:keySpec="ძ" />
            <Key latin:keySpec="X" />
            <Key latin:keySpec="ჩ" />
            <Key latin:keySpec="V" />
            <Key latin:keySpec="B" />
            <Key latin:keySpec="N" />
            <Key latin:keySpec="M" />
