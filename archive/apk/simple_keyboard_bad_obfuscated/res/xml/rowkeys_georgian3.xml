        <default>
            <Key latin:keySpec="ზ" />
            <Key latin:keySpec="ხ" latin:moreKeys="ჴ" />
            <Key latin:keySpec="ც" />
            <Key latin:keySpec="ვ" latin:moreKeys="ჳ" />
            <Key latin:keySpec="ბ" />
            <Key latin:keySpec="ნ" latin:moreKeys="ჼ" />
            <Key latin:keySpec="მ" />
