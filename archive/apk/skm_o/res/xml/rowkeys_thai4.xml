<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked">
            <Key latin:keyLabelFlags="fontDefault" latin:keySpec="(" />
            <Key latin:keyLabelFlags="fontDefault" latin:keySpec=")" />
            <Key latin:keySpec="ฉ" />
            <Key latin:keySpec="ฮ" />
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ฺ|ฺ" />
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ์|์" />
            <Key latin:keyLabelFlags="fontDefault" latin:keySpec="\?" />
            <Key latin:keySpec="ฒ" />
            <Key latin:keySpec="ฬ" />
            <Key latin:keySpec="ฦ" />
        </case>
        <default>
            <Key latin:keySpec="ผ" />
            <Key latin:keySpec="ป" />
            <Key latin:keySpec="แ" />
            <Key latin:keySpec="อ" />
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ิ|ิ" />
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ื|ื" />
            <Key latin:keySpec="ท" />
            <Key latin:keySpec="ม" />
            <Key latin:keySpec="ใ" />
            <Key latin:keySpec="ฝ" />
        </default>
    </switch>
</merge>