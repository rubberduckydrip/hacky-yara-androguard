<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSet="hindi_compact">
            <key-style latin:moreKeys="्" latin:styleName="moreKeySpecDevanagariSignVirama" />
            <key-style latin:moreKeys="ॅ,ऍ" latin:styleName="moreKeysDevanagariSignVirama" />
        </case>
        <case latin:keyboardLayoutSet="marathi">
            <key-style latin:moreKeys="अ" latin:styleName="moreKeysDevanagariSignVirama" />
        </case>
        <default>
            <key-style latin:styleName="moreKeysDevanagariSignVirama" />
        </default>
    </switch>
    <key-style latin:keySpec="्" latin:parentStyle="moreKeysDevanagariSignVirama" latin:styleName="baseKeyDevanagariSignVirama" />
</merge>