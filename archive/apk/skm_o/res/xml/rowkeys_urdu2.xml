<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked">
            <Key latin:keySpec="؏" />
            <Key latin:keySpec="؎" />
            <Key latin:keySpec="ؔ" />
            <Key latin:keySpec="ؒ" />
            <Key latin:keySpec="ؓ" />
            <Key latin:keySpec="ؑ" />
            <Key latin:keySpec="ؐ" />
            <Key latin:keySpec="ﷺ" />
            <Key latin:keySpec="ﷻ" />
