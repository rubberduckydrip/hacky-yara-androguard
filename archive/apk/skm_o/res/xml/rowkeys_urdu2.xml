            <Key latin:keyHintLabel="آ" latin:keySpec="ا" latin:moreKeys="آ,أ" />
            <Key latin:keyHintLabel="ص" latin:keySpec="س" latin:moreKeys="ص" />
            <Key latin:keyHintLabel="ڈ" latin:keySpec="د" latin:moreKeys="ڈ" />
            <Key latin:keySpec="ف" />
            <Key latin:keyHintLabel="غ" latin:keySpec="گ" latin:moreKeys="غ" />
            <Key latin:keyHintLabel="ھ" latin:keySpec="ح" latin:moreKeys="ھ" />
            <Key latin:keyHintLabel="ض" latin:keySpec="ج" latin:moreKeys="ض" />
            <Key latin:keyHintLabel="خ" latin:keySpec="ک" latin:moreKeys="خ" />
            <Key latin:keySpec="ل" />
        </default>
    </switch>
</merge>